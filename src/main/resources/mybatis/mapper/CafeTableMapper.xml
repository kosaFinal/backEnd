<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.com.example.demo.cafe.cafeTable.mapper.CafeTableMapper">

    <select id="getTableNumbers" resultType="string">
        SELECT tablenumber
        FROM cafetable
        WHERE cafeId={cafeId} AND tableType={tableType}
    </select>

</mapper>